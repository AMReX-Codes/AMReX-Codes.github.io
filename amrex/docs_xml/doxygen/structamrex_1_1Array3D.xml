        <name>sum</name>
        <qualifiedname>amrex::Array3D::sum</qualifiedname>
        <param>
          <type>int</type>
          <declname>axis</declname>
        </param>
        <param>
          <type>int</type>
          <declname>loc0</declname>
        </param>
